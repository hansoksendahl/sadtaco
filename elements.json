{
  "tag": "svg",
  "attr": {
      "id": "stage"
  },
  "style": {
    "position": "absolute",
    "width": "100%",
	  "height": "100%",
  },
  "children": [
    {
      "tag": "image",
      "attr": {
          "id": "taco",
          "xlink:href": "./assets/images/sadtaco2.svg"
      },
      "on": {
          "click": function () {
              var memory = prompt("What would you like me to remember?", localStorage.taco)
              if (memory !== null) localStorage.taco = memory
          }
      }
    }
  ]
}